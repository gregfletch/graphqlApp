{
  "name": "graphql-app",
  "version": "0.1.0",
  "scripts": {
    "audit": "yarn audit",
    "build": "ng build",
    "build:watch": "ng build --watch=true",
    "codecov": "./node_modules/.bin/codecov",
    "lint": "tsc --noEmit && eslint . --ext ts,json",
    "lint:fix": "tsc --noEmit && eslint . --ext ts,json --quiet --fix",
    "ng": "ng",
    "start": "ng s --host graphql.app.lvh.me",
    "start:watch": "ng s --watch=true --host graphql.app.lvh.me",
    "stylelint": "stylelint 'src/**/*.{css,scss}'",
    "stylelint:fix": "stylelint 'src/**/*.{css,scss}' --fix",
    "test": "ng test --watch=false --browsers=ChromeHeadless --codeCoverage",
    "test:watch": "ng test --watch=true --browsers=ChromeHeadless --codeCoverage"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.0.8",
    "@angular/cdk": "^11.0.3",
    "@angular/common": "^11.0.8",
    "@angular/compiler": "^11.0.8",
    "@angular/core": "^11.0.8",
    "@angular/forms": "^11.0.8",
    "@angular/material": "^11.0.3",
    "@angular/platform-browser": "^11.0.8",
    "@angular/platform-browser-dynamic": "^11.0.8",
    "@angular/router": "^11.0.8",
    "bootstrap": "^4.5.3",
    "jquery": "^3.5.1",
    "rxjs": "^6.6.3",
    "tslib": "^2.1.0",
    "zone.js": "^0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1100.6",
    "@angular-devkit/core": "^11.0.6",
    "@angular/cli": "^11.0.6",
    "@angular/compiler-cli": "^11.0.8",
    "@types/faker": "^5.1.5",
    "@types/jasmine": "^3.6.2",
    "@types/node": "^14.14.20",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "codecov": "^3.8.1",
    "codelyzer": "^6.0.1",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "faker": "^5.1.0",
    "fishery": "^1.0.1",
    "jasmine-core": "^3.6.0",
    "jasmine-spec-reporter": "^6.0.0",
    "karma": "^5.2.3",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "karma-spec-reporter": "^0.0.32",
    "prettier": "^2.2.1",
    "prettier-eslint": "^12.0.0",
    "stylelint": "^13.8.0",
    "stylelint-config-sass-guidelines": "^7.1.0",
    "ts-node": "^9.1.1",
    "typescript": "~4.0.5"
  }
}
